    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.4.0",
    "koa-send": "^4.1.3",
    "koa2-static-middleware": "^1.0.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
